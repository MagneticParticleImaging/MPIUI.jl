    <property name="upper">2000</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adjTracerVolume">
    <property name="upper">100</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
  </object>
  <object class="GtkTextBuffer" id="textBuffSeFo"/>
  <object class="GtkTextBuffer" id="txtBuffProtocolDescription"/>
  <object class="GtkBox" id="boxProtocol">
    <property name="visible">True</property>
    <property name="margin_top">4</property>
    <property name="margin_bottom">4</property>
    <property name="spacing">4</property>
    <child>
      <object class="GtkBox" id="boxProtocolTabVisu">
        <property name="visible">True</property>
        <property name="hexpand">1</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox" id="box15">
            <property name="visible">True</property>
            <child>
              <object class="GtkGrid">
                <property name="visible">True</property>
                <property name="width_request">180</property>
                <property name="valign">center</property>
                <property name="column_spacing">4</property>
                <child>
                  <object class="GtkLabel" id="lblProtocolName">
                    <property name="visible">True</property>
                    <property name="label" translatable="1">Default Protocol</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="btnPickProtocol">
                    <property name="visible">True</property>
                    <property name="focusable">1</property>
                    <property name="receives_default">1</property>
                    <layout>
                      <property name="column">0</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="tbRec">
                <property name="visible">True</property>
                <property name="css-classes">toolbar</property>
                <child>
                  <object class="GtkButton" id="tbInit">
                    <property name="visible">True</property>
                    <property name="tooltip_text" translatable="1">Initializes the current protocol with the current paramters</property>
                    <property name="label" translatable="1">Init</property>
                    <property name="use_underline">1</property>
                  </object>
                </child>
                <child>
                  <object class="GtkToggleButton" id="tbRun">
                    <property name="visible">True</property>
                    <property name="sensitive">0</property>
                    <property name="tooltip_text" translatable="1">Starts the current protocol</property>
                    <property name="label" translatable="1">Execute</property>
                    <property name="use_underline">1</property>
                  </object>
                </child>
                <child>
                  <object class="GtkToggleButton" id="tbPause">
                    <property name="visible">True</property>
                    <property name="sensitive">0</property>
                    <property name="tooltip_text" translatable="1">Attempts to pause the protocol</property>
                    <property name="label" translatable="1">Pause</property>
                    <property name="use_underline">1</property>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="tbCancel">
                    <property name="visible">True</property>
                    <property name="sensitive">0</property>
                    <property name="tooltip_text" translatable="1">Attempts to cancel the protocol</property>
                    <property name="label" translatable="1">Cancel</property>
                    <property name="use_underline">1</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="visible">True</property>
                <property name="hexpand">1</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkLabel" id="lbInfo">
                    <property name="visible">True</property>
                    <property name="label" translatable="1">Est. Runtime</property>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="lblRuntime">
                    <property name="visible">True</property>
                    <property name="label" translatable="1">N/A</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkProgressBar" id="pbProtocol">
                <property name="visible">True</property>
                <property name="text" translatable="1">x / y Steps</property>
                <property name="show_text">1</property>
                <property name="ellipsize">end</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkNotebook" id="nbDataWidgets">
            <property name="vexpand">1</property>
            <property name="focusable">1</property>
            <child>
              <placeholder/>
            </child>
            <child>
              <placeholder/>
            </child>
            <child>
              <placeholder/>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkScrolledWindow" id="wndProtoclParams">
        <property name="visible">True</property>
        <property name="hexpand">1</property>
        <property name="width_request">250</property>
        <property name="focusable">1</property>
        <!-- <property name="shadow_type">in</property> -->
        <property name="child">
          <object class="GtkViewport">
            <property name="visible">True</property>
            <property name="child">
              <object class="GtkBox">
                <property name="visible">True</property>
                <property name="margin-start">4</property>
                <property name="margin-end">4</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="1">Data Storage</property>
                    <attributes>
                      <attribute name="weight" value="bold"></attribute>
                      <attribute name="size" value="13000"></attribute>
                    </attributes>
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="boxStorageParams">
                    <property name="orientation">vertical</property>
                    <property name="spacing">2</property>
                    <child>
                      <placeholder/>
                    </child>
                    <child>
                      <placeholder/>
                    </child>
                    <child>
                      <placeholder/>
                    </child>
                    <child>
                      <placeholder/>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="label" translatable="1">Data Display</property>
                    <attributes>
                      <attribute name="weight" value="bold"></attribute>
                      <attribute name="size" value="13000"></attribute>
                    </attributes>
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="boxGUIParams">
                    <property name="orientation">vertical</property>
                    <property name="spacing">2</property>
                    <child>
                      <placeholder/>
