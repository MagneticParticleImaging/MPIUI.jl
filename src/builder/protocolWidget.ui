                    <child>
                      <placeholder/>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="boxProtocolParams">
                    <property name="orientation">vertical</property>
                    <property name="spacing">4</property>
                    <child>
                      <object class="GtkLabel">
                        <property name="visible">True</property>
                        <property name="label" translatable="1">Protocol</property>
                        <attributes>
                          <attribute name="weight" value="heavy"></attribute>
                          <attribute name="size" value="13000"></attribute>
                        </attributes>
                      </object>
                    </child>
                    <child>
                      <object class="GtkExpander">
                        <property name="visible">True</property>
                        <property name="focusable">1</property>
                        <child>
                          <object class="GtkGrid">
                            <property name="visible">True</property>
                            <child>
                              <object class="GtkLabel">
                                <property name="visible">True</property>
                                <property name="label" translatable="1">Type</property>
                                <layout>
                                  <property name="column">0</property>
                                  <property name="row">1</property>
                                </layout>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="lblProtocolType">
                                <property name="visible">True</property>
                                <layout>
                                  <property name="column">1</property>
                                  <property name="row">1</property>
                                </layout>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTextView" id="txtProtocolDescription">
                                <property name="visible">True</property>
                                <property name="focusable">1</property>
                                <property name="hexpand">1</property>
                                <property name="wrap_mode">word</property>
                                <property name="buffer">txtBuffProtocolDescription</property>
                                <layout>
                                  <property name="column">0</property>
                                  <property name="row">2</property>
                                  <property name="column-span">2</property>
                                </layout>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel">
                                <property name="visible">True</property>
                                <property name="label" translatable="1">Scanner</property>
                                <layout>
                                  <property name="column">0</property>
                                  <property name="row">0</property>
                                </layout>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="lblScannerName">
                                <property name="visible">True</property>
                                <layout>
                                  <property name="column">1</property>
                                  <property name="row">0</property>
                                </layout>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child type="label">
                          <object class="GtkLabel">
                            <property name="visible">True</property>
                            <property name="label" translatable="1">Description</property>
                            <attributes>
                              <attribute name="weight" value="bold"></attribute>
                            </attributes>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkExpander">
                        <property name="visible">True</property>
                        <property name="focusable">1</property>
                        <property name="hexpand">1</property>
                        <property name="vexpand">0</property>
                        <property name="expanded">1</property>
                        <child>
                          <object class="GtkBox" id="boxProtocolParameter">
                            <property name="visible">True</property>
                            <property name="hexpand">1</property>
                            <property name="vexpand">1</property>
                            <property name="orientation">vertical</property>
                            <child>
